{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Real Tx",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/repo-uploader/src/index.ts",
        "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
        "sourceMaps": true,
        "envFile": "${workspaceFolder}/.env",
        "outFiles": ["${workspaceFolder}/**/*.ts"],
        "internalConsoleOptions": "openOnSessionStart",
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ]
      },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Test Tx",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/repo-uploader/src/testTx.ts",
      "runtimeArgs": ["-r", "ts-node/register/transpile-only"],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "outFiles": ["${workspaceFolder}/**/*.ts"],
      "internalConsoleOptions": "openOnSessionStart",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
    ]
  }
  